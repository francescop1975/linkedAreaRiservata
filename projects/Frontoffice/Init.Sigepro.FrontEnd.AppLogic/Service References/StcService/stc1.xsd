					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="unbounded" name="localizzazione" type="tns:LocalizzazioneNelComuneType">
        <annotation>
          <documentation>
						Indica la localizzazione dell'intervento. Si
						potrebbero aggiungere anche i dati catastali.
					</documentation>
        </annotation>
      </element>
      <element name="statoPratica" type="tns:StatoPraticaType" />
    </sequence>
  </complexType>
  <complexType name="RichiedenteType">
    <sequence>
      <element minOccurs="0" maxOccurs="1" name="ruolo" type="tns:RuoloType" />
      <element name="anagrafica" type="tns:PersonaFisicaType" />
    </sequence>
  </complexType>
  <complexType name="ProcuraType">
    <sequence>
      <element name="cfRappresentato" type="string" />
      <element name="cfProcuratore" type="string" />
      <element minOccurs="0" maxOccurs="1" name="procura" type="tns:DocumentiType" />
      <element minOccurs="0" maxOccurs="1" name="documentoIdentita" type="tns:DocumentiType" />
    </sequence>
  </complexType>
