      <element minOccurs="0" maxOccurs="1" name="fabbricato" type="string" />
      <element minOccurs="0" maxOccurs="1" name="km" type="string" />
      <element minOccurs="0" maxOccurs="1" name="frazione" type="tns:FrazioneType" />
      <element minOccurs="0" maxOccurs="1" name="circoscrizione" type="tns:CircoscrizioneType" />
      <element minOccurs="0" maxOccurs="1" name="quartiere" type="tns:QuartiereType" />
      <element minOccurs="0" maxOccurs="unbounded" name="riferimentoCatastale" type="tns:RiferimentoCatastaleType" />
      <element minOccurs="0" maxOccurs="1" name="coordinate" type="tns:CoordinateType" />
      <element minOccurs="0" maxOccurs="1" name="tipo" type="tns:TipoLocalizzazioneType" />
      <element minOccurs="0" maxOccurs="1" name="uuid" type="string" />
      <element minOccurs="0" maxOccurs="1" name="cap" type="string" />
