      <enumeration value="V_1_11" />
      <enumeration value="V_1_12" />
      <enumeration value="V_1_13" />
    </restriction>
  </simpleType>
  <simpleType name="NaturaFoType">
    <restriction base="string">
      <enumeration value="comunicazione" />
      <enumeration value="ordinario" />
      <enumeration value="scia" />
    </restriction>
  </simpleType>
  <complexType name="DettaglioAttivitaType">
    <sequence>
      <element name="idPratica" type="string" />
      <element name="idAttivita" type="string" />
      <element minOccurs="0" maxOccurs="1" name="dataAttivita" type="date" />
      <element minOccurs="0" name="oraDataAttivita">
        <simpleType>
          <restriction base="string">
            <pattern value="[0-2][0-9]:[0-5][0-9]" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" maxOccurs="1" name="dataScadenza" type="date" />
      <element minOccurs="0" name="numeroProtocolloGenerale" type="string">
        <annotation>
          <documentation> E' il protocollo generale riferito all'attivita'
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="dataProtocolloGenerale" type="date" />
      <element name="tipoAttivita" type="tns:TipoAttivitaType">
        <annotation>
          <documentation> Indicare l'attivita' da notificare Es. 'Richiesta
						di parere' o 'Comunicazione parere'
					</documentation>
        </annotation>
      </element>
      <element name="esito" type="boolean">
        <annotation>
          <documentation> Indica se l'attivita' ha avuto un esito positivo o
						meno Es. 'Invio parere' o 'Rilascio atto/autorizzazione'. Nelle
						attivita' che non prevedono esito indicare sempre true.
						Es. 'Comunicazione atti'
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="parere" type="string">
        <annotation>
          <documentation> E' il parere nel caso di notifica attivita'
						'Comunicazione parere'.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="note" type="string" />
      <element minOccurs="0" maxOccurs="unbounded" name="altriDati" type="tns:ParametroType" />
      <element minOccurs="0" maxOccurs="unbounded" name="documenti" type="tns:DocumentiType">
        <annotation>
          <documentation> E' la lista dei documenti allegati all'attivita'
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="unbounded" name="procedimenti" type="tns:ProcedimentoType" />
      <element minOccurs="0" maxOccurs="unbounded" name="documentiDaSostituire" type="tns:SostituzioniDocumentaliType" />
    </sequence>
  </complexType>
  <complexType name="TipoAttivitaType">
    <sequence minOccurs="1">
      <element name="codice" type="string" />
      <element name="descrizione" type="string" />
    </sequence>
  </complexType>
  <complexType name="RiferimentiAttivitaType">
    <sequence>
      <element name="idPratica" type="string" />
      <element minOccurs="0" name="idProcedimento" type="string" />
      <element name="idAttivita" type="string" />
      <element minOccurs="0" name="numeroProtocolloGenerale" type="string">
        <annotation>
          <documentation> Va indicato solo se si tratta di protocollo
						generale e non di
						settore/direzione.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="dataProtocolloGenerale" type="date" />
      <element minOccurs="0" maxOccurs="unbounded" name="altriDati" type="tns:ParametroType" />
    </sequence>
  </complexType>
  <complexType name="AltriSoggettiType">
    <sequence>
      <element name="tipoRapporto" type="tns:RuoloType" />
      <element name="soggetto" type="tns:AnagrafeType" />
      <element minOccurs="0" maxOccurs="1" name="anagraficaCollegata" type="tns:AnagrafeType" />
    </sequence>
  </complexType>
  <complexType name="AnagrafeType">
    <choice minOccurs="1">
      <element name="personaFisica" type="tns:PersonaFisicaType" />
      <element name="personaGiuridica" type="tns:PersonaGiuridicaType" />
    </choice>
  </complexType>
  <complexType name="ComuneType">
    <annotation>
      <documentation> Sara' possibile decodificare il comune attraverso
				l'indicazione del
				CodiceCatastale (Es.D612), del CodiceIstat (Es.
				048017) o del nome del comune (Es.
				Firenze)
			</documentation>
    </annotation>
    <choice minOccurs="1">
      <element name="codiceCatastale" type="string" />
      <element name="codiceIstat" type="string" />
      <element name="comune" type="string" />
    </choice>
  </complexType>
  <complexType name="InterventoType">
    <sequence>
      <element minOccurs="0" name="codice" type="string" />
      <element name="descrizione" type="string" />
    </sequence>
  </complexType>
  <complexType name="IscrizioneRegistroType">
