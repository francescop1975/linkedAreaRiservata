          </restriction>
        </simpleType>
      </element>
      <element name="foglio" type="string" />
      <element name="particella" type="string" />
      <element minOccurs="0" maxOccurs="1" name="sub" type="string" />
      <element minOccurs="0" maxOccurs="1" name="sezione" type="string" />
      <element minOccurs="0" maxOccurs="1" name="unitaImobiliare" type="string" />
    </sequence>
  </complexType>
  <complexType name="RiferimentiPraticaType">
    <sequence>
      <element minOccurs="0" maxOccurs="1" name="idPratica" type="string" />
      <element minOccurs="0" maxOccurs="1" name="numeroPratica" type="string" />
      <element minOccurs="0" maxOccurs="1" name="dataPratica" type="date" />
      <element minOccurs="0" name="oraDataPratica">
        <simpleType>
          <restriction base="string">
            <pattern value="[0-2][0-9]:[0-5][0-9]" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="numeroProtocolloGenerale" type="string">
        <annotation>
          <documentation> Va indicato il numero di protocollo della pratica
						solo in caso di integrazione
						con il protocollo generale (non va
						specificato nulla se il protocollo e' di
						settore/direzione).
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="dataProtocolloGenerale" type="date" />
      <element minOccurs="0" maxOccurs="unbounded" name="altriDati" type="tns:ParametroType" />
    </sequence>
  </complexType>
  <complexType name="DettaglioPraticaType">
    <sequence>
      <element name="idPratica" type="string">
        <annotation>
          <documentation>
						E' l'identificativo univoco della pratica
						all'interno del backoffice mittente.
					</documentation>
        </annotation>
      </element>
      <element name="numeroPratica" type="string">
        <annotation>
          <documentation>
						E' il numero della pratica leggibile dagli
						operatori.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="1" name="codicePraticaTelematica" type="string" />
      <element name="dataPratica" type="date" />
      <element minOccurs="0" name="oraDataPratica">
        <simpleType>
          <restriction base="string">
            <pattern value="[0-2][0-9]:[0-5][0-9]" />
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="numeroProtocolloGenerale" type="string">
        <annotation>
          <documentation>
						Va indicato il numero di protocollo della
						pratica solo in caso di integrazione con il
						protocollo generale (non va specificato nulla se
						il protocollo e' di settore/direzione).
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="dataProtocolloGenerale" type="date" />
      <element minOccurs="0" maxOccurs="1" name="codiceComune" type="tns:ComuneType" />
      <element minOccurs="0" maxOccurs="1" name="codiceAccreditamentoSUAP" type="string">
        <annotation>
          <documentation>
						Va indicato il codice di accreditamento suap
						configurato nel portale impresainungiorno. Usato
						in caso di software Sportello unico.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="1" name="domicilioElettronico" type="string">
        <annotation>
          <documentation>
						E' il domicilio elettronico di riferimento
						(tipicamente PEC) per ricevere comunicazioni in
						merito alla pratica da parte dei
						richiedenti/professionisti.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="richiedente" type="tns:RichiedenteType">
        <annotation>
          <documentation>
						Il richiedente e' colui che presenta la domanda.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="1" name="aziendaRichiedente" type="tns:PersonaGiuridicaType" />
      <element minOccurs="0" maxOccurs="unbounded" name="procure" type="tns:ProcuraType" />
      <element minOccurs="0" maxOccurs="1" name="intermediario" type="tns:AnagrafeType">
        <annotation>
          <documentation>
						E' il tecnico esterno o l'associazione che
						presenta la domanda per conto del richiedente.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="unbounded" name="altriSoggetti" type="tns:AltriSoggettiType">
        <annotation>
          <documentation>
						Eventuale lista di soggetti collegati alla
						pratica: soci, curatori fallimentari o in caso
						di edilizia direttore lavori o responsabile
						della sicurezza.
					</documentation>
        </annotation>
      </element>
      <element name="oggetto" type="string">
        <annotation>
          <documentation>
						E' necessario descrivere bene l'oggetto che
						rappresenta la pratica.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="1" name="insegna" type="string" />
      <element minOccurs="0" name="intervento" type="tns:InterventoType">
        <annotation>
          <documentation>
						E' l'intervento codificato che i due backoffice
						mittente e destinatario decidono di comunicarsi,
						specifica la codifica del tipo di pratica.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="unbounded" name="localizzazione" type="tns:LocalizzazioneNelComuneType">
        <annotation>
          <documentation>
						Indica la localizzazione dell'intervento. Si
						potrebbero aggiungere anche i dati catastali.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="unbounded" name="documenti" type="tns:DocumentiType">
        <annotation>
          <documentation>
						E' la lista dei documenti a corredo della
						pratica. Domanda, dichiarazioni e comunicazioni
						per il richiedente.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="unbounded" name="altriDati" type="tns:ParametroType" />
      <element minOccurs="0" maxOccurs="unbounded" name="procedimenti" type="tns:ProcedimentoType" />
      <element minOccurs="0" maxOccurs="unbounded" name="schede" type="tns:SchedaType" />
      <element minOccurs="0" maxOccurs="unbounded" name="oneri" type="tns:OneriType" />
      <element minOccurs="0" name="annotazioni" type="string">
        <annotation>
          <documentation>
						Eventuali informazioni aggiuntive o note sulla
						pratica.
					</documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="1" name="naturaFo" type="tns:NaturaFoType">
        <annotation>
          <documentation>
						la natura del procedimento che viene impostato dal frontend
					</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
